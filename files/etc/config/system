
config system
	option hostname 'UPS'
	option timezone 'CST-8'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server 'ntp.aliyun.com'

config led
	option sysfs 'tp-link:blue:system'
	option default '1'
	option trigger 'usbport'
	list port 'usb1-port1'
	option name 'USB'

